package statistics;

/**
 * Statistics Exception class to indicate exceptions from the Statistics Generator.
 * @author Alicia Bendz
 *
 */
@SuppressWarnings("serial")
public class StatisticsException extends Exception {
	
	/**
	 * Constructor that takes a message and creates an Exception with this message.
	 * @param s The message to use.
	 */
	public StatisticsException(String s){
		super(s);
	}

}

package DataAccess;

import java.util.Date;

/**
 * Abstract class that represents a single feedback
 * @author lineker
 *
 */
public class Feedback {
	int mId;
	String mText;
	String mSource;
	Date mDate;
	
	/**
	 * Setter Id
	 * @param id
	 */
	public void setId(int id)
	{
		mId = id;
	}
	
	/**
	 * Getter Id
	 * @return
	 */
	public int getId()
	{
		return mId;
	}
	
	/**
	 * Getter Text
	 * @return
	 */
	public String getText()
	{
		return mText;
	}
	
	/**
	 * Setter Text
	 * @param text
	 */
	public void setText(String text)
	{
		mText = text;
	}	
	
	/**
	 * Setter Source
	 * @param text
	 */
	public void setSource(String src)
	{
		mSource = src;
	}
	
	/**
	 * Getter Source
	 * @return
	 */
	public String getSource()
	{
		return mSource;
	}
	
	/**
	 * Setter Date
	 * @param text
	 */
	public void setDate(Date date)
	{
		mDate = date;
	}
	
	/**
	 * Getter Date
	 * @return
	 */
	public Date getDate()
	{
		return mDate;
	}
}
